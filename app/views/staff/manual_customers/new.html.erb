<p id="notice"><%= notice %></p>

<h1>Customer Management</h1>

<div class="row">

<div class="col-lg-12">
  <div class="ibox float-e-margins">
     <div class="ibox-title">
      <h5>Customer Management </h5>
      <div class="ibox-tools">
       <a class="collapse-link">
        <i class="fa fa-chevron-up"></i>
       </a>
      </div>
     </div>
     <div class="ibox-content">
      <div class="row">


      </div>

      <%= simple_form_for(@manual_customer, :url => '/partner/manualcustomer/', html: { :style => 'display: inline', class: 'form-horizontal', :multipart => true}) do |f| %>
  <%= f.input :first_name, class: 'span1', label: 'First Name', error: ' is mandatory, please specify one' %>
  <%= f.input :last_name, class: 'span1', label: 'Last Name', error: ' is mandatory, please specify one' %>
  <%= f.input :email, label: 'Email', error: ' is mandatory, please specify one' %>
  <%= f.input :phone, label: 'Phone', error: ' is mandatory, please specify one' %>
  <%= f.input :address1, label: 'Address Line 1', error: ' is mandatory, please specify one' %>
  <%= f.input :address2, label: 'Address Line 2', error: ' is mandatory, please specify one' %>
  <%= f.input :city, label: 'City', error: ' is mandatory, please specify one' %>
  <%= f.input :country, label: 'Country', error: ' is mandatory, please specify one' %>
  <%= f.input :zipcode, label: 'Postcode', error: ' is mandatory, please specify one' %>
  <%= f.input :notes, label: 'Notes', error: ' is mandatory, please specify one' %>
  <%= f.button :submit %>
  <% end %>

</div>
</div>
</div>
</div>
