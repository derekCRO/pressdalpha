<h1> </h1>
<div class="row wrapper border-bottom white-bg page-heading">
      <div class="col-lg-10">
          <h2>Category</h2>
          
      </div>
      <div class="col-lg-2">

      </div>
</div>

<div class="wrapper wrapper-content">
       	<div class="row">
       	<% @category.errors.full_messages.each do |msg| %>
			    <li class="error msg"><%= msg %></li>
		    <% end %>
				<!-----Left Side Start------->
				<div class="col-md-4">
					<div class="ibox float-e-margins">
						<div class="ibox-title">
							<h5>Category Image</h5>
						</div>
						<div>
							<div class="ibox-content no-padding border-left-right pt-20 pl-20">
								<%= image_tag @category.image.url(:small), :class => 'img-responsive' %>
								
							</div>
						</div>
					</div>
				</div>
				
				
				<div class="col-md-7">
		           <div class="ibox float-e-margins">
		               <div class="ibox-title">
		                   <h5>Edit Category </h5>
		                   <div class="ibox-tools">
		                       <a class="collapse-link">
		                           <i class="fa fa-chevron-up"></i>
		                       </a>
		                       
		                       
		                   </div>
		               </div>
		               <div class="ibox-content">
		                   <div class="row">
		                       <div class="col-sm-12 b-r">
		                       	  <%= render 'formedit', category: @category %>
		                           
		                       </div>
		                   </div>
		               </div>
				   </div>
				</div>
				<!-----Left Side End------->
				
				
		
       </div>
</div>

