<% @catattributes.each do |catattribute| %>
<%= form_for(catattribute, :url => partner_subupdate_category_path(catattribute.id), :html => {:class => 'form-horizontal'}) do |f|  %>

    
	<div class="form-group"><label class="col-sm-2 control-label" for="category_name">Name</label>
	 	<div class="col-sm-10"><%= f.text_field :name, :class => 'form-control', :required => 'required' %></div>
	</div>

	<div class="form-group"><label class="col-sm-2 control-label" for="category_name">Hour per item</label>
	 	<div class="col-sm-10"><%= f.text_field :hour_per_item, :class => 'form-control', :required => 'required' %></div>
	</div>

	<div class="form-group"><label class="col-sm-2 control-label" for="category_name">Material Cost</label>
	 	<div class="col-sm-10"><%= f.text_field :material_cost, :class => 'form-control', :required => 'required' %></div>
	</div>
	
  
	  <div class="form-group">
		 <div class="col-sm-4 col-sm-offset-2">
		     <button class="btn btn-primary" type="submit" name="commit" value="update" data-disable-with="Save">Save</button>
		     <%= link_to 'Back', admin_sub_category_path(@parent), :class=>'btn btn-white' %>
		 </div>
	  </div>
	  
	<div class="hr-line-dashed"></div>
<% end %>
<% end %>