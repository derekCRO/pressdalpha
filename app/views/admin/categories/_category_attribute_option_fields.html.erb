<div class="nested-fields">
	<h5> Options for Dropdown </h5>
	<div class="form-group"><label class="col-sm-2 control-label" for="options">Options</label>
	 	<div class="col-sm-10"><%= f.text_field :options, :class => 'form-control' %></div>
	</div>
  <div class="form-group"><label class="col-sm-2 control-label" for="option_hours">Hours</label>
	 	<div class="col-sm-10"><%= f.text_field :option_hours, :class => 'form-control' %></div>
	</div>
  <div class="form-group"><label class="col-sm-2 control-label" for="option_price">Prices</label>
	 	<div class="col-sm-10"><%= f.text_field :option_price, :class => 'form-control' %></div>
	</div>
  <%= link_to_remove_association 'Remove', f, class: "btn btn-danger btn-xs" %>
</div>
