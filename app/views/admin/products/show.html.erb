<div class="page-heading">
            <h3>
                Company Detail
            </h3>
            
			<p id="notice"><%= notice %></p>
</div>
<div class="wrapper">
        <div class="row">
			<div class="col-sm-12">
				<section class="panel">
					<header class="panel-heading">
						Details
					</header>
					<div class="panel-body">


<p>
  <strong>Title:</strong>
  <%= @product.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @product.description %>
</p>

<p>
  <strong>Serial:</strong>
  <%= @product.serial %>
</p>

<p>
  <strong>Weight:</strong>
  <%= @product.weight %>
</p>

<p>
  <strong>Pt weight:</strong>
  <%= @product.pt_weight %>
</p>

<p>
  <strong>Pd weight:</strong>
  <%= @product.pd_weight %>
</p>


<p>
  <strong>Rh weight:</strong>
  <%= @product.rh_weight %>
</p>



<p>
  <strong>Moisture:</strong>
  <%= @product.moisture %>
</p>

<p>
  <strong>Category:</strong>
  <%= @product.category.name %>
  
</p>

<p>
  <strong>Make:</strong>
  <%= @product.make %>
</p>

<p>
  <strong>Model:</strong>
  <%= @product.model %>
</p>

<p>
  <strong>Stainless Steel:</strong>
  <%= @product.stainless_steel %>
</p>

<p>
  <strong>Image url:</strong>
  <%= image_tag @product.image.url(:small) %>
</p>

<%= link_to 'Edit', edit_admin_product_path(@product), :class=>'btn btn-primary' %>
<%= link_to 'Back', admin_products_path, :class=>'btn btn-primary' %>
					</div>
				</section>
			</div>
		</div>
</div>
