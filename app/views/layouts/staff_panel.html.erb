<!DOCTYPE html>
<html>
  <head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<meta name="keywords" content="admin, dashboard, bootstrap, template, flat, modern, theme, responsive, fluid, retina, backend, html5, css, css3">
	<meta name="description" content="">
	<meta content='<%= staff_signed_in? ? current_staff.id : "" %>' name='user-id'/>
	<title>Staff Admin</title>
	<%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'adminpanel' %>
  <%= javascript_include_tag 'admin/js/jquery/jquery-3.1.1.min' %>
  <%= javascript_include_tag 'adminpanel' %>
  <%= javascript_include_tag 'admin/js/misc/moment.min' %>
  <%= javascript_include_tag "admin/js/highcharts", "chartkick" %>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
 <%= yield(:head) %>
  </head>

  <body>
	<div class="container-fluid">
      <div class="row">
      <%= render :partial => 'layouts/staffsidepanel' %>
      <div class="right-column">
      	  <%= render :partial => 'layouts/stafftopbar' %>
          <%= yield %>
      </div>
     </div>
     </div>

     <div id="modal-window" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
       <div class="modal-dialog" role="document">
         <div class="modal-content"></div>
       </div>
     </div>

     <%= javascript_include_tag 'admin/js/bootstrap.min' %>
     <%= javascript_include_tag 'admin/js/popper.min' %>
     <%= javascript_include_tag 'admin/js/mdb.min' %>
     <%= javascript_include_tag 'admin/js/fullcalendar.min' %>
     <%= javascript_include_tag 'admin/js/scripts' %>
     <%= javascript_include_tag 'admin/js/scripts-frontend' %>
     <%= javascript_include_tag 'admin/js/misc/holder.min' %>
     <%= javascript_include_tag 'admin/js/misc/ie10-viewport-bug-workaround' %>
     <%= javascript_include_tag 'admin/js/misc/jquery.easing.1.3' %>
     <%= javascript_include_tag 'admin/js/datatables.min' %>
     <%= javascript_include_tag 'admin/js/plugins/chartjs/chart.bundle.min' %>
     <%= javascript_include_tag 'admin/js/plugins/custom-scrollbar/jquery.mCustomScrollbar.concat.min' %>
     <%= javascript_include_tag 'select2.min' %>
     <%= javascript_include_tag 'rateyo.min' %>


  </body>

</html>
